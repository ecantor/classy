<h1>Feedback Submissions</h1>

<table>
  <tr>
    <th>Source</th>
    <th>Message</th>
    <th></th>
    <th></th>
    <th></th>
    <th>ID</th>
    <th>Zip Code</th>
    <th></th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.source %></td>
    <td><%= feedback.message %></td>
    <td><%= feedback.msid %></td>
    <td><%= feedback.zip %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

